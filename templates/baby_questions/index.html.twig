{% extends 'base.html.twig' %}

{% block title %}BabyQuestions index{% endblock %}

{% block body %}
    <h1>BabyQuestions index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Question</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for baby_question in baby_questions %}
            <tr>
                <td>{{ baby_question.id }}</td>
                <td>{{ baby_question.question }}</td>
                <td>
                    <a href="{{ path('app_baby_questions_show', {'id': baby_question.id}) }}">show</a>
                    <a href="{{ path('app_baby_questions_edit', {'id': baby_question.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_baby_questions_new') }}">Create new</a>
{% endblock %}
